{
  "name": "onokori",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "7.2.1"
  },
  "scripts": {
    "postinstall": "typings install",
    "start": "node ./server/bin/www",
    "tsc:w": "tsc -w",
    "bundle": "browserify ./_tmp/index.js -t babelify -o server/public/js/index.js -d",
    "bundle:w": "watchify ./_tmp/index.js -t babelify -o server/public/js/index.js -v -d",
    "lint": "eslint ./server/*.js ./server/routes/** ./front/** ./front/**/**",
    "test": "NODE_ENV=test mocha --require intelli-espower-loader --recursive",
    "test:w": "NODE_ENV=test mocha --require intelli-espower-loader --recursive --reporter dot --watch",
    "minify": "uglifyjs server/public/js/index.js -o server/public/js/index.js",
    "build": "tsc && mkdirp ./server/public/js && npm run bundle && npm run minify",
    "watch":"npm-run-all --parallel bundle:w test:w tsc:w",
    "heroku-postbuild": "sequelize db:migrate --config server/config/config.js --migrations-path ./server/migrations/ && npm run build"
  },
  "dependencies": {
    "axios": "^0.13.1",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-3": "^6.11.0",
    "babelify": "^7.3.0",
    "body-parser": "~1.15.1",
    "browserify": "^13.0.1",
    "compression": "^1.6.2",
    "connect-redis": "^3.1.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "express": "~4.13.4",
    "express-session": "^1.14.0",
    "flumpt": "^0.2.0",
    "jade": "~1.11.0",
    "morgan": "~1.7.0",
    "passport": "^0.3.2",
    "passport-twitter": "^1.0.4",
    "pg": "^6.0.3",
    "pg-hstore": "^2.3.2",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "sequelize": "^3.23.6",
    "sequelize-cli": "^2.4.0",
    "serve-favicon": "~2.3.0",
    "stone-skin": "^0.6.0",
    "twit": "^2.2.4",
    "typescript": "^2.0.0",
    "typings": "^1.3.2",
    "uglify-js": "^2.7.0"
  },
  "devDependencies": {
    "eslint": "^3.0.1",
    "eslint-plugin-react": "^5.2.2",
    "intelli-espower-loader": "^1.0.1",
    "mocha": "^2.5.3",
    "npm-run-all": "^2.3.0",
    "power-assert": "^1.4.1",
    "sinon": "^1.17.4",
    "supertest": "^1.2.0",
    "watchify": "^3.7.0"
  }
}
