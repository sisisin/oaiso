{
  "name": "oaiso",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "engines": {
    "node": "7.2.1"
  },
  "scripts": {
    "clean": "rm -rf _tmp/ _tmp_aot/ server/public/js/*",
    "start": "node --harmony ./server/bin/www",
    "ngc": "ngc -p tsconfig.aot.json",
    "bundle": "webpack -p",
    "bundle:dev": "webpack --config webpack.config.dev.js -d",
    "bundle:w": "webpack --config webpack.config.dev.js -w",
    "lint": "npm run lint:server && npm run lint:front",
    "lint:server": "eslint ./server/*.js ./server/routes/**",
    "lint:front": "tslint \"src/**/*.ts\"",
    "test": "npm run lint && npm run ut",
    "ut": "npm run ut:server",
    "ut:server": "NODE_ENV=test mocha --require intelli-espower-loader --recursive",
    "ut:server:w": "NODE_ENV=test mocha --require intelli-espower-loader --recursive --reporter dot --watch",
    "build": "npm run ngc && npm run bundle",
    "build:dev": "npm run bundle:dev",
    "watch:s": "npm-run-all --parallel ut:server:w",
    "watch:m": "jade docs/mocks/ -o docs/mocks_dist/ -w",
    "watch:f": "npm-run-all --parallel bundle:w",
    "migrate": "sequelize db:migrate --config server/config/config.js --migrations-path ./server/migrations/",
    "heroku-postbuild": "npm run migrate && npm run build"
  },
  "dependencies": {
    "@angular/common": "^2.3.1",
    "@angular/compiler": "^2.3.1",
    "@angular/compiler-cli": "^2.3.1",
    "@angular/core": "^2.3.1",
    "@angular/forms": "^2.3.1",
    "@angular/http": "^2.3.1",
    "@angular/platform-browser": "^2.3.1",
    "@angular/platform-browser-dynamic": "^2.3.1",
    "@angular/router": "^3.3.1",
    "@types/jasmine": "^2.5.38",
    "@types/node": "^6.0.52",
    "body-parser": "~1.15.1",
    "compression": "^1.6.2",
    "connect-redis": "^3.1.0",
    "cookie-parser": "~1.4.3",
    "core-js": "^2.4.1",
    "debug": "~2.2.0",
    "express": "~4.13.4",
    "express-session": "^1.14.0",
    "jade": "~1.11.0",
    "morgan": "~1.7.0",
    "passport": "^0.3.2",
    "passport-twitter": "^1.0.4",
    "pg": "^6.0.3",
    "pg-hstore": "^2.3.2",
    "rxjs": "^5.0.0-rc.4",
    "sequelize": "^3.23.6",
    "sequelize-cli": "^2.4.0",
    "serve-favicon": "~2.3.0",
    "stone-skin": "^0.6.0",
    "ts-helpers": "^1.1.1",
    "twit": "^2.2.4",
    "typescript": "~2.0.10",
    "webpack": "^2.2.0-rc.1",
    "zone.js": "^0.7.2"
  },
  "devDependencies": {
    "angular2-template-loader": "^0.6.0",
    "eslint": "^3.0.1",
    "intelli-espower-loader": "^1.0.1",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-remap-istanbul": "^0.2.1",
    "jquery": "^3.1.1",
    "materialize-css": "^0.97.8",
    "mocha": "^2.5.3",
    "npm-run-all": "^2.3.0",
    "power-assert": "^1.4.1",
    "raw-loader": "^0.5.1",
    "sinon": "^1.17.4",
    "supertest": "^1.2.0",
    "ts-loader": "^1.3.3",
    "tslint": "^4.0.2"
  }
}
