{
  "name": "oaiso",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "engines": {
    "node": "7.2.1"
  },
  "scripts": {
    "start": "node --harmony ./server/bin/www",
    "bundle": "webpack -d",
    "bundle:w": "webpack -w",
    "lint": "npm run lint:server && npm run lint:front",
    "lint:server": "eslint ./server/*.js ./server/routes/**",
    "lint:front": "tslint \"src/**/*.ts\"",
    "test": "npm run lint && npm run ut",
    "ut": "npm run test:server",
    "test:server": "NODE_ENV=test mocha --require intelli-espower-loader --recursive",
    "test:server:w": "NODE_ENV=test mocha --require intelli-espower-loader --recursive --reporter dot --watch",
    "watch": "npm-run-all --parallel bundle:w test:w tsc:w",
    "watch:m": "jade docs/mocks/ -o docs/mocks_dist/ -w",
    "build": "webpack -p",
    "watch:s": "npm-run-all --parallel test:server:w",
    "watch:f": "npm-run-all --parallel bundle:w",
    "heroku-postbuild": "sequelize db:migrate --config server/config/config.js --migrations-path ./server/migrations/ && npm run build"
  },
  "dependencies": {
    "@angular/common": "2.2.3",
    "@angular/compiler": "2.2.3",
    "@angular/core": "2.2.3",
    "@angular/forms": "2.2.3",
    "@angular/http": "2.2.3",
    "@angular/platform-browser": "2.2.3",
    "@angular/platform-browser-dynamic": "2.2.3",
    "@angular/router": "3.2.3",
    "core-js": "^2.4.1",
    "rxjs": "5.0.0-beta.12",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.6.23",
    "body-parser": "~1.15.1",
    "compression": "^1.6.2",
    "connect-redis": "^3.1.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "express": "~4.13.4",
    "express-session": "^1.14.0",
    "jade": "~1.11.0",
    "morgan": "~1.7.0",
    "passport": "^0.3.2",
    "passport-twitter": "^1.0.4",
    "pg": "^6.0.3",
    "pg-hstore": "^2.3.2",
    "sequelize": "^3.23.6",
    "sequelize-cli": "^2.4.0",
    "serve-favicon": "~2.3.0",
    "stone-skin": "^0.6.0",
    "twit": "^2.2.4",
    "typescript": "^2.0.0"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.33",
    "eslint": "^3.0.1",
    "intelli-espower-loader": "^1.0.1",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-remap-istanbul": "^0.2.1",
    "jquery": "^3.1.1",
    "materialize-css": "^0.97.8",
    "mocha": "^2.5.3",
    "npm-run-all": "^2.3.0",
    "power-assert": "^1.4.1",
    "sinon": "^1.17.4",
    "supertest": "^1.2.0",
    "ts-loader": "^1.3.3",
    "tslint": "^4.0.2",
    "webpack": "^1.14.0"
  }
}